{
	scrollStart=scroll;
	do !wait for not-touch
	{
		if (startY~=1024)
		{
			@get_touch $1A -> y; ! y
			scroll=scrollStart+(startY-y)/3;
			if (scroll>0)
			{
				scroll=0;
			}
			else if (scroll<-BufferHeight+KeyboardStartLine-StatusHeight)
			{
				scroll=-BufferHeight+KeyboardStartLine-StatusHeight;
			}
