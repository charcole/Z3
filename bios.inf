		@print "  q  |   w  |   e   |  r   |   t   |   y  |   u   |   i   |  o   |   p^";
		@print "      a  |   s   |  d   |   f   |   g  |   h   |  j   |   k   |   l^";
		@print "          z  |   x   |  c   |   v  |   b   |  n   |  m   |   ,^";
