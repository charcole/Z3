{
	if (text->1=='m' && text->2=='a' && text->3=='n' && text->4=='d' && text->5=='e'
	 && text->6=='l' && text->7=='b' && text->8=='r' && text->9=='o' && text->10=='t')
	{
		parse0=Mandelbrot();
		i=1;
	}
}
else if (text->0==6)
{
	if (text->1=='i' && text->2=='n' && text->3=='v' && text->4=='a' && text->5=='d' && text->6=='e')
	{
		parse0=SpaceInvaders();
		i=1;
	}
}
if (i)
{
	text->0=4;
	text->1='w';
	text->2='a';
	text->3='i';
	text->4='t';
	text->5=0;
}

parse0=parse;
parse++;
parse++;

parse0->1=0;
count=text->0;
i=1;
while (count)
{
	c=text->i;
	if (c==32)
	{
		sep=-1;
	}
	else
	{
		dict=DictionaryOff-->0;
		sep=dict->0;
		while (sep)
		{
			if (c==dict->sep)
				break;
			sep--;
		}
	}
	if (readingWord)
	{
		if (sep)
		{
			parse0->1=parse0->1+1;
			parse->2=i-parse->3;
			temp=FindInDictionary(text, parse);
			parse=parse+4;
			readingWord=0;
			if (sep>0)
			{
				parse->3=i;
